{
  "name": "artui",
  "version": "0.0.0-semantically-released",
  "description": "A modern React + TypeScript component library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rimraf dist",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:js": "vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/art-ui.git"
  },
  "keywords": [
    "react",
    "typescript",
    "component-library",
    "ui",
    "design-system"
  ],
  "author": "Georgii Kharlampiiev",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/addon-links": "^9.0.0",
    "@storybook/react-vite": "^9.0.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-storybook": "^9.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^22.0.0",
    "storybook": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^0.34.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
